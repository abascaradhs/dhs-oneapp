{
  "name": "oneapp-server",
  "version": "0.0.1",
  "description": "GraphQL API for OneApp",
  "main": "index.js",
  "bin": "index.js",
  "private": true,
  "scripts": {
    "start": "yarn serve",
    "serve": "node index.js",
    "serve:mock": "node index.js --mock | pino-pretty",
    "live": "nodemon --inspect index.js | pino-pretty",
    "lint:pre": "eslint '**/*.js' --ignore-pattern node_modules/ --ignore-pattern dist/ --no-fix",
    "lint": "eslint '**/*.js' --ignore-pattern node_modules/ --ignore-pattern dist/ --fix",
    "format": "prettier-eslint --write",
    "pkg": "pkg . --out-path dist",
    "test": "LOG_ENABLED=false jest"
  },
  "dependencies": {
    "accept-language-parser": "^1.5.0",
    "apollo-server-express": "^2.18.2",
    "cfenv": "^1.2.3",
    "config": "^3.3.2",
    "datasource-sql": "^1.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "generate-password": "^1.5.1",
    "graphql": "^15.3.0",
    "graphql-constraint-directive": "^2.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.2",
    "graphql-parse-resolve-info": "^4.9.0",
    "graphql-rate-limit": "^2.0.1",
    "graphql-shield": "^7.3.7",
    "graphql-tools": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.7",
    "lodash": "^4.17.19",
    "nodemailer": "^6.4.14",
    "oracledb": "file:lib/oracledb-5.0.0.tgz",
    "pino": "^6.7.0",
    "require-directory": "^2.1.1",
    "string-template": "^1.0.0",
    "yargs": "^16.1.0",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "apollo-server-testing": "^2.18.2",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.5.3",
    "nodemon": "^2.0.5",
    "pino-pretty": "^4.3.0",
    "pkg": "^4.4.9",
    "prettier-eslint-cli": "^5.0.0"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}
