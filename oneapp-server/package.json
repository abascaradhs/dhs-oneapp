{
  "name": "oneapp-server",
  "version": "0.0.1",
  "description": "GraphQL API for OneApp",
  "main": "index.js",
  "bin": "index.js",
  "private": true,
  "scripts": {
    "serve": "node index.js",
    "serve:mock": "node index.js --mock | pino-pretty",
    "live": "nodemon index.js | pino-pretty",
    "lint:pre": "eslint '**/*.js' --ignore-pattern node_modules/ --ignore-pattern dist/ --no-fix",
    "lint": "eslint '**/*.js' --ignore-pattern node_modules/ --ignore-pattern dist/ --fix",
    "format": "prettier-eslint --write",
    "pkg": "pkg . --out-path dist",
    "test": "jest"
  },
  "dependencies": {
    "apollo-server-express": "^2.16.1",
    "config": "^3.3.1",
    "datasource-sql": "^1.3.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^15.3.0",
    "graphql-constraint-directive": "^2.0.3",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.3",
    "graphql-tools": "^6.0.18",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "lodash": "^4.17.19",
    "oracledb": "^5.0.0",
    "pino": "^6.5.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.3.0",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.1.0",
    "pkg": "^4.4.9",
    "prettier-eslint-cli": "^5.0.0"
  }
}
